{
  "atma": {
    "plugins": [
      "atma-loader-ts"
    ],
    "settings": {
      "include": {
        "amd": true,
        "extentionDefault": {
          "js": "ts"
        },
        "routes": {
          "@core": "src/{0}",
          "@dequanto": "dequanto/src/{0}",
          "@0xweb": "0xweb/{0}"
        }
      },
      "atma-loader-ts": {
        "typescript": {
          "compilerOptions": {
            "module": "AMD",
            "sourceMap": false,
            "experimentalDecorators": true,
            "esModuleInterop": true,
            "allowSyntheticDefaultImports": true,
            "target": "ES2020"
          }
        }
      }
    }
  },
  "dependencies": {
    "@ethereumjs/common": "^2.5.0",
    "@ethereumjs/tx": "^3.3.2",
    "a-di": "^1.0.25",
    "alot": "^0.2.96",
    "appcfg": "^0.4.63",
    "atma": "^0.15.12",
    "atma-io": "^1.2.59",
    "atma-utils": "^0.2.51",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "ethers": "^5.5.1",
    "everlog": "^1.0.8",
    "memd": "^0.3.10",
    "papaparse": "^5.3.1",
    "ts-nameof": "^5.0.0",
    "web3": "^1.6.0",
    "web3-core": "^1.6.0",
    "web3-core-subscriptions": "^1.6.0"
  },
  "devDependencies": {
    "atma-loader-ts": "^1.1.18",
    "includejs": "^0.16.4"
  }
}
